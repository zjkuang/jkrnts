{
  "name": "jkrnts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:devices": "emulator -list-avds",
    "android:revokepermissions:location": "adb shell pm revoke com.jkrnts android.permission.ACCESS_COARSE_LOCATION && adb shell pm revoke com.jkrnts android.permission.ACCESS_FINE_LOCATION && adb shell pm revoke com.jkrnts android.permission.ACCESS_BACKGROUND_LOCATION",
    "android:sim:uninstall": "adb uninstall com.jkrnts",
    "ios": "react-native run-ios",
    "ios:devices": "xcrun simctl list",
    "ios:sim:ipad-9th": "react-native run-ios --simulator \"iPad (9th generation)\"",
    "ios:sim:uninstall": "xcrun simctl uninstall booted org.reactjs.native.example.jkrnts",
    "pod": "pod install --project-directory=ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "string-gen": "cd src/assets/strings && echo y | npx -y @zjkuang/i18next-wizard && cd ../../..",
    "gen": "yarn string-gen"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-navigation/native": "^6.0.11",
    "@zjkuang/react-native-utils": "^0.2.19",
    "i18next": "^21.8.14",
    "intl-pluralrules": "^1.3.1",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "mobx-state-tree": "^5.1.5",
    "mst-persist": "^0.1.3",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-yet-another-utils": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.22.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
